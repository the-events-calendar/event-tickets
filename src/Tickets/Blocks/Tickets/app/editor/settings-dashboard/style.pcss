.tribe-editor__settings-dashboard__header-left {

	.tribe-editor__tickets__settings-dashboard--loading & {

		& > svg {

			&,
			& path {
				fill: #AEB4BB;
			}
		}
	}
}

.tribe-editor__settings-dashboard__header-left-text {

	.tribe-editor__tickets__settings-dashboard--loading & {
		color: #AEB4BB;
	}
}
