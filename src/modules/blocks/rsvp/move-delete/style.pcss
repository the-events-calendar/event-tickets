.tribe-editor__rsvp__content-row--move-delete {
	padding: 30px 0 0;

	& > button {
		font-size: 15px !important;
		line-height: 18px;
		padding: 0;

		&:first-child {
			color: #009fd4;

			&:after {
				color: #8d949b;
				content: '|';
				margin: 0 10px;
			}
		}

		&:last-child {
			color: #da394d;
		}
	}
}
