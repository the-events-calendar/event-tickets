/**
 * Attendees block view styles
 *
 * @since  4.9
 */
.tribe-block__attendees {
	margin-bottom: 30px;
	margin-top: 30px;

	img {
		border-radius: 100%;
		display: inline-block;
		height: 60px;
		width: 60px;
	}
}

.tribe-block__attendees__title {
	display: block;
	font-family: 'Helvetica Neue', Helvetica, -apple-system, BlinkMacSystemFont, Roboto, Arial, sans-serif;
	font-size: 21px;
	font-style: normal;
	font-weight: bold;
	line-height: normal;
}
